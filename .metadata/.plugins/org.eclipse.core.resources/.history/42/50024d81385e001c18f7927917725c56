package interaction;

public class Seller {
	private int money;
	private int mango;
	
	public Seller() {
		int money = 0;
		int mango = 10;
	}
	
	public void sellMango(int mango) {
		money = (money * mango) - mango;
		if (mango<=0) {
			System.out.println("망고가 모자랍니다.");
			return;
		}
	}
	
	public void showSeller() {
		
	}

}
