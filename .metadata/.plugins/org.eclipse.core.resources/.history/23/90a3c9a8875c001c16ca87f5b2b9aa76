package overloading;

public class Calulator {
	
	public void plus (int a) {
		System.out.println(a+"에 1을 더한 값은"+);
	}
	public void plus (int a, int b) {
		System.out.println(); 
	}
	public void plus (double a) {
		System.out.println();
	}
	public void plus (String eifng) {
		System.out.println("문자열은 연산 대상이 아닙니다.");
	}

}
