package interaction;

public class Main01 {

	public static void main(String[] args) {
		// 판매자 생성
		Seller s1 = new Seller(30); // 상인1
		Seller s2 = new Seller(20); // 상인2
		// 구매자 생성
		Buyer b1 = new Buyer(100000); // 구매자1
		
		s1.showSeller(); // 상인1정보
		s2.showSeller(); // 상인2정보
		b1.showBuyer(); // 구매자정보

	}

}
