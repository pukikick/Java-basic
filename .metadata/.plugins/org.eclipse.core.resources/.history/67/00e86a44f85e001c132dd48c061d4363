package game.polymorphism;

public class Commoner {
	// 속성(필드, 변수) 값으로 name, hp, mp, atk를 추가해주세요.
	private String name;
	private int hp;
	private int mp;
	private int atk;

	// 생성자로 위 필드를 초기화하도록 설정해주세요.
	public Commoner(String name, int hp, int mp, int atk) {
		this.name=name;
		this.hp=hp;
		this.mp=mp;
		this.atk=atk;
	}
	
	
	
}
